{
  "name": "@svene/openapi-ts-cli",
  "private": false,
  "publisher": "svene",
  "version": "1.0.4",
  "description": "A CLI to generate TypeScript types from an OpenAPI specification",
  "main": "src/index.js",
  "bin": {
    "@svene/openapi-ts-cli": "src/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write ."
  },
  "keywords": [
    "openapi",
    "typescript",
    "types",
    "generator"
  ],
  "author": "Aidre Cabrera",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aidrecabrera/openapi-ts"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "inquirer": "^9.0.0",
    "openapi-typescript": "^4.0.0",
    "winston": "^3.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.4"
  }
}
